{
  "version": "1.0.0",
  "workspace": {
    "name": "Qt-Theme-Studio",
    "type": "python",
    "sourceControl": {
      "enabled": true,
      "provider": "git",
      "autoRefresh": true,
      "showChanges": true
    },
    "indexing": {
      "enabled": true,
      "excludePatterns": [
        "venv/**",
        ".git/**",
        "__pycache__/**",
        "*.pyc",
        ".pytest_cache/**",
        ".mypy_cache/**"
      ]
    }
  }
}
